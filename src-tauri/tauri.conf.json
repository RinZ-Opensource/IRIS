{
  "productName": "IRIS",
  "version": "0.1.0",
  "identifier": "com.rinz.iris",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "IRIS",
        "width": 1280,
        "height": 720,
        "fullscreen": true,
        "decorations": false,
        "resizable": false,
        "dragDropEnabled": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": ["nsis"],
    "resources": []
  }
}

